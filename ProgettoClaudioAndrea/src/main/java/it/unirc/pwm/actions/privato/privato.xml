			<result name="input">/WEB-INF/privato/modificaCliente.jsp</result>
		</action>
		
		<action name="RichiediIndirizzoSpedizione">
			<result>/WEB-INF/privato/indirizzospedizione/visualizzaIndirizziSpedizione.jsp</result>
		</action>
		
